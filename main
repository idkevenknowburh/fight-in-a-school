local ChangeButton = MainTab:CreateButton({
   Name = "Change Weapons/Skin Stats",
   Callback = function()
   local _batspeed = 1.85
local madebyspxifyerbat = game.Players.LocalPlayer
local _plrs1 = game:GetService("Players")
local _gthr = _plrs1:GetPlayers()

local _weaponSkin = game:GetService("ReplicatedStorage").Skins
local m4deb4spxifyerbat = game:GetService("ReplicatedStorage").Weapons
local _takeDowns = game:GetService("ReplicatedStorage").Takedowns

Spx4fyermadeDisBat = require(m4deb4spxifyerbat)
WeaponOnMe = require(_weaponSkin)
takeMeDown = require(_takeDowns)

print(Spx4fyermadeDisBat)
print(WeaponOnMe)
print(_takeDowns)

for i,v in pairs(_gthr) do

gg = v.Character.HumanoidRootPart
lp = madebyspxifyerbat.Name

local function _pause()
wait(2)
end

print(" Editing Weapon's And Skin's Stats... ")
Spx4fyermadeDisBat.Bat.Speed = _batspeed
Spx4fyermadeDisBat.Bat.Damage = 6
print(" Finished Bat! ")
_pause()
Spx4fyermadeDisBat["Boxing Gloves"].Damage = 40.5
Spx4fyermadeDisBat["Boxing Gloves"].BloodEnabled = true
print(" Finished Boxing Gloves... Speed Couldn't Be Changed :( ")
_pause()
Spx4fyermadeDisBat["Brass Knuckles"].Damage = 7
Spx4fyermadeDisBat["Brass Knuckles"].Speed = 2


WeaponOnMe["Nightmare Club"].Damage = 7
WeaponOnMe["Nightmare Club"].Critical_Damage = 40.5
print("Done!")
end
   end,
})

local AntiKnockoutToggle = MainTab:CreateToggle({
   Name = "Anti-Knockout",
   CurrentValue = false,
   Flag = "Toggle1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
   local bysh4k4r6o = game:GetService("Players").LocalPlayer
local ondiscord = bysh4k4r6o.Character or bysh4k4r6o.CharacterAdded:Wait()
local forreal = ondiscord:WaitForChild("Knocked")

spawn(function()
while wait(1) do
game:GetService("RunService").Heartbeat:Connect(function()
    if forreal then
        forreal.Value = false
    end
end)
end
end)
   end,
})

local AutoFarmButton = MainTab:CreateButton({
   Name = "Auto-Farm Respect (This can't be disabled yet, so rejoin to stop and re-execute / Be sure you are using a ALT, this is for alt accounts, have another main use the spawn command in Infinite Yield)",
   Callback = function()
   local plr = game.Players.LocalPlayer
    local isFarming = false  -- Flag to track farming state

    -- Function to handle teleportation and farming
    local function teleportPlayer()
        local char = plr.Character or plr.CharacterAdded:Wait()
        local root = char:WaitForChild("HumanoidRootPart")

        if not root or not char:FindFirstChild("Humanoid") then
            return
        end

        coroutine.wrap(function()
            while isFarming do
                if root and char.Parent then
                    root.CFrame = CFrame.new(-74, 4, -4)  -- Teleport to the specified coordinates
                    local humanoid = char:FindFirstChild("Humanoid")
                    if humanoid then
                        wait(3)
                        humanoid.Health = 0  -- Die by setting health to 0
                    end
                end
                wait(3)
            end
        end)()
    end
   end,
})

local ButtonAntiAFK = MiscTab:CreateButton({
   Name = "Load Anti-AFK",
   Callback = function()
   local VirtualUser = game:GetService('VirtualUser')
 
game:GetService('Players').LocalPlayer.Idled:Connect(function()
    VirtualUser:CaptureController()
    VirtualUser:ClickButton2(Vector2.new())
end)
   end,
})

